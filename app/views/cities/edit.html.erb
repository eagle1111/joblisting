<div class="col-md-8 col-centered">


<div class="panel panel-info">
  <div class="panel-heading"><h1>Edit City</h1></div>
  <div class="panel-body">
    <%= simple_form_for(@city) do |f| %>
    <%= f.error_notification %>
    <%= f.input :name %>
    <%= f.submit "Submit", class: "btn btn-danger" %>|<%= link_to 'Back', cities_path %>
  <% end %>
  </div>
</div>
</div>
