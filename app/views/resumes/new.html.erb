<div class="col-md-10 col-centered">
  <div class="panel panel-info">
    <div class="panel-heading"><h3> 投交履历到 <%= @job.title %> </h3></div>
      <div class="panel-body">
        <%= simple_form_for [@job, @resume] do |f| %>
        <%= f.input :content %>
        <%= f.input :attachment %>
        <%= f.submit "送出", class: "btn btn-danger" %>
        <% end %>
      </div>
    </div>
  </div>
